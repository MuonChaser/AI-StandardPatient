# Flask 配置
FLASK_DEBUG=True
FLASK_HOST=0.0.0.0
FLASK_PORT=5000
SECRET_KEY=your-secret-key-here

# AI模型配置
API_KEY=your-openai-api-key-here
MODEL_BASE=https://api.openai.com/v1
MODEL_NAME=gpt-3.5-turbo

# CORS配置（多个域名用逗号分隔）
CORS_ORIGINS=*

# 会话配置
MAX_SESSIONS=100
SESSION_TIMEOUT=3600

# 其他配置
ENVIRONMENT=development
